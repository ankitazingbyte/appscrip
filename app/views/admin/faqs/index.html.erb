<p id="notice"><%= notice %></p>

<h1>Faq</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>SubTitle</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @faqs.each do |faq| %>
      <tr>
        <td><%= faq.title %></td>
        <td><%= faq.subtitle %></td>
        <td><%= link_to 'Show', admin_faq_path(faq) %></td>
        <td><%= link_to 'Edit', edit_admin_faq_path(faq) %></td>
        <td><%= link_to 'Destroy', admin_faq_url(faq), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fontpage', new_admin_faq_path %>
